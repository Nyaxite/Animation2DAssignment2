package objects{	import starling.display.Sprite;	import starling.display.Image;	import starling.display.BlendMode;	import starling.core.Starling;	public class Background extends Sprite	{		private var background:Image;		public function Background()		{			// Load our Ocean's image from our Texture Atlas			background = new Image(Assets.backgroundTexture);			background.blendMode = BlendMode.NONE;			// display our Ocean on the stage			addChild(background);		}	}}